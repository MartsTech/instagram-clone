{
  "name": "instagram-clone",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "gen-env": "npx gen-env-types .env.local -o src/types/env.d.ts -e .",
    "prepare": "husky install",
    "format": "prettier --write ./src",
    "check-format": "prettier --check ./src",
    "check-lint": "eslint ./src --ext ts --ext tsx",
    "check-types": "tsc --pretty --noEmit",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types && yarn test && yarn build"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.4",
    "faker": "^5.5.3",
    "firebase": "^9.1.2",
    "mobx": "^6.3.3",
    "mobx-react-lite": "^3.2.1",
    "next": "11.1.2",
    "next-seo": "^4.28.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-toastify": "^8.0.3"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.3.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@types/faker": "^5.5.8",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "autoprefixer": "^10.3.7",
    "eslint": ">=5.16.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.1",
    "gen-env-types": "^1.3.0",
    "husky": "^7.0.0",
    "jest": "^27.2.5",
    "mutationobserver-shim": "^0.3.7",
    "postcss": "^8.3.9",
    "prettier": "^2.4.1",
    "react-spinners": "^0.11.0",
    "tailwind-scrollbar": "^1.3.1",
    "tailwind-scrollbar-hide": "^1.0.3",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.4"
  }
}
